{
  "Args": [
    "__sapper__/build/index.js"
  ],
  "Dirs": [
    "./node_modules",
    "./tests/node/no-dependencies/src/__sapper__/build"
  ],
  "Env": {
    "NODE_DEBUG": "*"
  },
